    "rxjs": "^6.3.2",
    "styled-components": "^3.4.9"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 6",
    "yarn": ">= 1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix --ext .js,.jsx,.ts,.tsx"
  },
  "resolutions": {
    "babel-core": "^7.0.0-0"
  }
}
