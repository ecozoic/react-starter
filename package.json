{
  "name": "react-starter",
  "version": "6.0.0",
  "description": "A scaffold for React+Redux+Rxjs UIs powered by Webpack",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "pretest": "yarn clean:test",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "tdd": "concurrently \"yarn start\" \"yarn test:watch\"",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "prebuild": "yarn clean:build",
    "build": "cross-env NODE_ENV=production webpack",
    "serve": "dotenv pushstate-server dist",
    "clean": "yarn clean:test && yarn clean:build",
    "clean:test": "rimraf coverage",
    "clean:build": "rimraf dist reports",
    "cz": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/react-starter.git"
  },
  "author": "ecozoic <johnnn.mitchell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecozoic/react-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/react-starter#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@storybook/addon-a11y": "^4.0.0-alpha.21",
    "@storybook/addon-actions": "^4.0.0-alpha.21",
    "@storybook/addon-backgrounds": "^4.0.0-alpha.21",
    "@storybook/addon-centered": "^4.0.0-alpha.21",
    "@storybook/addon-info": "^4.0.0-alpha.21",
    "@storybook/addon-knobs": "^4.0.0-alpha.21",
    "@storybook/addon-links": "^4.0.0-alpha.21",
    "@storybook/addon-notes": "^4.0.0-alpha.21",
    "@storybook/addon-storysource": "^4.0.0-alpha.21",
    "@storybook/addon-viewport": "^4.0.0-alpha.21",
    "@storybook/react": "^4.0.0-alpha.21",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.10.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/storybook__addon-a11y": "^3.3.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-backgrounds": "^3.2.1",
    "@types/storybook__addon-centered": "^3.3.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__addon-links": "^3.3.2",
    "@types/storybook__addon-notes": "^3.3.3",
    "@types/storybook__react": "^3.0.9",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.7.1",
