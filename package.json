{
  "name": "react-starter",
  "version": "1.1.3",
  "description": "A starter kit for React+Redux UIs",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "test": "npm run test:client && npm run test:server",
    "pretest:client": "npm run clean:test:client",
    "test:client": "cross-env NODE_ENV=test jest --config jest.client.conf.json --coverage",
    "tdd:client": "cross-env NODE_ENV=test jest --config jest.client.conf.json --watch",
    "pretest:server": "npm run clean:test:server",
    "test:server": "cross-env NODE_ENV=test jest --config jest.server.conf.json --coverage",
    "tdd:server": "cross-env NODE_ENV=test jest --config jest.server.conf.json --watch",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production webpack --progress --profile --colors --bail",
    "serve": "cross-env NODE_ENV=production node src/server/main.js",
    "serve:dev": "cross-env NODE_ENV=development PORT=3000 nodemon --inspect src/server/main.js",
    "pree2e": "npm run clean:e2e && selenium-standalone install",
    "e2e": "cross-env NODE_ENV=e2e nightwatch",
    "flow": "flow check",
    "flow-typed": "flow-typed",
    "predocs": "npm run clean:docs",
    "docs": "jsdoc -c jsdoc.conf.json",
    "clean:build": "rimraf dist",
    "clean:docs": "rimraf docs",
    "clean:test": "rimraf coverage",
    "clean:test:client": "rimraf coverage/client",
    "clean:test:server": "rimraf coverage/server",
    "clean:e2e": "rimraf reports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/react-starter.git"
  },
  "author": "John Mitchell <johnnn.mitchell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecozoic/react-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/react-starter#readme",
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "eslint": "3.14.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "2.0.0-rc.0",
    "file-loader": "0.10.0",
    "flow-bin": "0.38.0",
    "flow-typed": "2.0.0",
    "html-webpack-plugin": "2.28.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "18.1.0",
    "jsdoc": "3.4.3",
    "jsdoc-babel": "0.3.0",
    "nightwatch": "0.9.12",
    "node-sass": "4.4.0",
    "nodemon": "1.11.0",
    "postcss-cssnext": "2.9.0",
    "postcss-loader": "1.2.2",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools": "3.3.2",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.2.0",
    "redux-logger": "2.7.4",
    "rimraf": "2.5.4",
    "sass-lint": "1.10.2",
    "sass-loader": "4.1.1",
    "sasslint-webpack-plugin": "1.0.4",
    "selenium-standalone": "6.0.0",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.2.1",
    "webpack-merge": "2.6.1"
  },
  "dependencies": {
    "core-js": "2.4.1",
    "immutable": "3.8.1",
    "koa": "1.2.4",
    "koa-compress": "1.0.9",
    "koa-conditional-get": "1.0.3",
    "koa-etag": "2.1.1",
    "koa-favicon": "1.2.1",
    "koa-logger": "1.3.1",
    "koa-mount": "1.3.0",
    "koa-send": "3.3.0",
    "koa-static": "2.0.0",
    "normalize.css": "5.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "4.0.0-alpha.6",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-immutable": "3.0.11"
  },
  "engines": {
    "node": "6.9.3",
    "npm": "3.10.10",
    "yarn": "0.19.1"
  }
}
