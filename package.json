{
  "name": "react-starter",
  "version": "6.0.0",
  "description": "A scaffold for React+Redux+Rxjs UIs powered by Webpack",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "pretest": "rimraf coverage",
    "test": "npm run jest -- --no-cache --coverage",
    "test:watch": "npm run jest -- --watch",
    "tdd": "concurrently \"npm start\" \"npm run test:watch\"",
    "prebuild": "rimraf dist && rimraf build",
    "build": "cross-env NODE_ENV=production webpack",
    "serve": "dotenv pushstate-server dist",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:js": "eslint --ext .js,.jsx src",
    "predocs": "rimraf docs",
    "jest": "cross-env NODE_ENV=test jest",
    "docs": "jsdoc -c jsdoc.conf.json",
    "predeploy": "cross-env BASENAME=/react-starter/ npm run build",
    "deploy": "gh-pages -d dist",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/react-starter.git"
  },
  "author": "ecozoic <johnnn.mitchell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecozoic/react-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/react-starter#readme",
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.3.0",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "jsdoc": "^3.5.5",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.1.0",
    "stylelint-webpack-plugin": "^0.10.3",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.1.3",
    "dotenv": "^5.0.0",
    "dotenv-cli": "^1.4.0",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "pushstate-server": "^3.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "rxjs": "^5.5.6"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  }
}
