{
  "name": "react-starter",
  "version": "6.0.0",
  "description": "A scaffold for React+Redux+Rxjs UIs powered by Webpack",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "pretest": "yarn clean:test",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "tdd": "concurrently \"yarn start\" \"yarn test:watch\"",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "prebuild": "yarn clean:build",
    "build": "cross-env NODE_ENV=production webpack",
    "serve": "dotenv pushstate-server dist",
    "clean": "yarn clean:test && yarn clean:build",
    "clean:test": "rimraf coverage",
    "clean:build": "rimraf dist reports",
    "cz": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/react-starter.git"
  },
  "author": "ecozoic <johnnn.mitchell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecozoic/react-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/react-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "commitizen": "^2.10.1",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "deep-freeze": "^0.0.1",
    "dotenv": "^6.0.0",
    "dotenv-cli": "^1.4.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flux-standard-action": "^2.0.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "pushstate-server": "^3.0.1",
    "react-test-renderer": "^16.4.2",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3",
    "webpack": "^4.17.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.3.2",
    "styled-components": "^3.4.5"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 6",
    "yarn": ">= 1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix --ext .js,.jsx,.ts,.tsx"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
