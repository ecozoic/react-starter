{
  "name": "react-starter",
  "version": "0.6.2",
  "description": "A starter kit for React+Redux UIs",
  "scripts": {
    "pretest": "npm run clean:test",
    "test": "cross-env NODE_ENV=test jest --config jest.conf.json",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production webpack --progress --profile --colors --bail",
    "serve": "cross-env NODE_ENV=production node server.js",
    "pree2e": "npm run clean:e2e && selenium-standalone install",
    "e2e": "cross-env NODE_ENV=e2e nightwatch",
    "flow": "flow check",
    "flow-typed": "flow-typed",
    "predocs": "npm run clean:docs",
    "docs": "jsdoc -c jsdoc.conf.json",
    "clean:build": "rimraf dist",
    "clean:docs": "rimraf docs",
    "clean:test": "rimraf coverage",
    "clean:e2e": "rimraf reports",
    "rimraf": "rimraf",
    "cross-env": "cross-env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/react-starter.git"
  },
  "author": "John Mitchell <johnnn.mitchell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecozoic/react-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/react-starter#readme",
  "devDependencies": {
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.9",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-react": "6.8.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "flow-bin": "0.37.0",
    "flow-typed": "2.0.0",
    "html-webpack-plugin": "2.24.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "17.0.3",
    "jsdoc": "3.4.3",
    "jsdoc-babel": "0.3.0",
    "nightwatch": "0.9.9",
    "node-sass": "3.13.0",
    "postcss-cssnext": "2.9.0",
    "postcss-loader": "1.2.1",
    "react-addons-test-utils": "15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.1.1",
    "rimraf": "2.5.4",
    "sass-lint": "1.10.2",
    "sass-loader": "4.0.2",
    "sasslint-webpack-plugin": "1.0.4",
    "selenium-standalone": "5.9.0",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.28",
    "webpack-dev-server": "2.1.0-beta.12",
    "webpack-merge": "1.1.1"
  },
  "dependencies": {
    "core-js": "2.4.1",
    "koa": "1.2.4",
    "koa-compress": "1.0.9",
    "koa-conditional-get": "1.0.3",
    "koa-etag": "2.1.1",
    "koa-favicon": "1.2.1",
    "koa-logger": "1.3.0",
    "koa-mount": "1.3.0",
    "koa-send": "3.2.0",
    "koa-static": "2.0.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "4.4.6",
    "react-router": "4.0.0-alpha.6",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0"
  },
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.8"
  }
}
