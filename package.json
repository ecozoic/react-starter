{
  "name": "react-starter",
  "version": "3.0.1",
  "description": "A starter kit for React+Redux UIs",
  "scripts": {
    "start": "concurrently \"yarn serve:dev\" \"yarn start:dev\"",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "test": "yarn test:client && yarn test:server",
    "pretest:client": "yarn clean:test:client",
    "test:client": "cross-env NODE_ENV=test jest --config jest.client.conf.json --no-cache --coverage",
    "tdd:client": "cross-env NODE_ENV=test jest --config jest.client.conf.json --watch",
    "pretest:server": "yarn clean:test:server",
    "test:server": "cross-env NODE_ENV=test jest --config jest.server.conf.json --no-cache --coverage",
    "tdd:server": "cross-env NODE_ENV=test jest --config jest.server.conf.json --watch",
    "tdd": "concurrently \"yarn serve:dev\" \"yarn start:dev\" \"yarn tdd:server\" \"yarn tdd:client \"",
    "prebuild": "yarn clean:build",
    "build": "cross-env NODE_ENV=production webpack --progress --profile --colors --bail",
    "serve": "cross-env NODE_ENV=production node src/server/main.js",
    "serve:dev": "cross-env NODE_ENV=development nodemon --inspect src/server/main.js",
    "predocs": "yarn clean:docs",
    "docs": "typedoc --out docs src/client --exclude \"**/*.spec.+(ts|tsx)\" --mode file",
    "clean:build": "rimraf dist",
    "clean:docs": "rimraf docs",
    "clean:test": "rimraf coverage",
    "clean:test:client": "rimraf coverage/client",
    "clean:test:server": "rimraf coverage/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/react-starter.git"
  },
  "author": "John Mitchell <johnnn.mitchell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecozoic/react-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/react-starter#readme",
