{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "interface-name": [
      false
    ],
    "member-access": [
      false
    ],
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false
  }
}
