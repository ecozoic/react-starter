{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "rules": {
    "import-name": false,
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ]
  }
}
